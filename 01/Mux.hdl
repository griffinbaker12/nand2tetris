/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in= sel, out= notSel);
    And(a= a, b= notSel, out= w1);
    And(a= b, b= sel, out= w2);
    Xor(a = w1, b = w2, out = out);
}