/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    And(a= a, b= b, out= w1);
    Not(in= w1, out= w2);
    Or(a = a, b= b, out = w3);
    And(a= w2, b= w3, out= out);
}